{
  "name": "EsJS Express App",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:0-18",
  "features": {
    "ghcr.io/NicoVIII/devcontainer-features/pnpm:2": {}
  },
  "portsAttributes": {
    "3000": {
      "label": "Aplicaci√≥n",
      "onAutoForward": "openPreview"
    }
